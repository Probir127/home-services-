[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/grbs-backend
ExecStart=/var/www/grbs-backend/.venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/var/www/grbs-backend/grbs_project.sock grbs_project.wsgi:application

[Install]
WantedBy=multi-user.target
